IF NOT DEFINED CLASSPATH GOTO NOCP
SET _OLDPATH=%CLASSPATH%
GOTO SETCP
:NOCP
SET _OLDPATH=
SET CLASSPATH=.
:SETCP

IF NOT DEFINED NETPATH GOTO NONP
SET _NP=%NETPATH%
GOTO SETNP
:NONP
SET _NP=.
:SETNP

:JHOME
SET _JAVA_BIN=
IF NOT DEFINED JAVA_HOME GOTO START
SET _JAVA_BIN=%JAVA_HOME%\bin\

:START
