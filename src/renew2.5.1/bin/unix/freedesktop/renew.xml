<?xml version="1.0" encoding="iso-8859-15"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-renew">
         <sub-class-of type="text/plain"/>
         <comment>Renew document</comment>
         <comment xml:lang="de">Renew-Dokument</comment>
  </mime-type>
  <mime-type type="application/x-renew-net">
         <sub-class-of type="application/x-renew"/>
         <comment>Renew Petri net drawing</comment>
         <comment xml:lang="de">Renew-Petrinetz</comment>
         <magic priority="50">
                <match type="string" value="de.renew.gui.CPNDrawing" offset="5:35"/>
         </magic>
         <glob pattern="*.rnw"/>
  </mime-type>
  <mime-type type="application/x-renew-interactiondiagram">
         <sub-class-of type="application/x-renew"/>
         <comment>Renew interaction diagram</comment>
         <comment xml:lang="de">Renew-Interaktionsdiagramm</comment>
         <magic priority="50">
                <match type="string" value="de.renew.diagram.dragram.DiagramDrawing" offset="5:50"/>
         </magic>
         <glob pattern="*.aip"/>
  </mime-type>
  <mime-type type="application/x-renew-drawing">
         <sub-class-of type="application/x-renew"/>
         <comment>Renew simple drawing</comment>
         <comment xml:lang="de">Renew-Zeichnung</comment>
         <magic priority="50">
                <match type="string" value="CH.ifa.draw.standard.StandardDrawing" offset="5:45"/>
         </magic>
         <glob pattern="*.draw"/>
  </mime-type>
  <mime-type type="application/x-renew-autosave">
         <sub-class-of type="application/x-renew"/>
         <comment>Renew document (autosaved)</comment>
         <comment xml:lang="de">Renew-Dokument (autom. gesichert)</comment>
         <glob pattern="*.aut"/>
  </mime-type>
  <mime-type type="application/x-renew-sns">
         <comment>Renew shadow net system</comment>
         <comment xml:lang="de">Renew-Schattennetzsystem</comment>
         <magic priority="50">
                <match type="string" value="de.renew.shadow.ShadowNetSystem" offset="5:50"/>
         </magic>
         <glob pattern="*.sns"/>
  </mime-type>
  <mime-type type="application/x-renew-state">
         <comment>Renew simulation state</comment>
         <comment xml:lang="de">Renew-Simulationszustand</comment>
         <magic priority="50">
                <match type="string" value="RenewState" offset="5:30"/>
         </magic>
         <glob pattern="*.rst"/>
  </mime-type>
  <mime-type type="application/pnml+xml">
         <comment>Renew PNML net drawing</comment>
         <comment xml:lang="de">Renew-PNML Petrinetz</comment>
	 <magic priority="50">
                <match type="string" value="pnml" offset="0:5"/>
         </magic>
         <glob pattern="*.pnml"/>
  </mime-type>
  <mime-type type="application/x-renew-lola">
         <comment>Renew Lola net drawing</comment>
         <comment xml:lang="de">Renew-Lola Petrinetz</comment>
         <magic priority="50">
                <match type="string" value="RenewState" offset="5:30"/>
         </magic>
         <glob pattern="*.net"/>
  </mime-type>
</mime-info>
